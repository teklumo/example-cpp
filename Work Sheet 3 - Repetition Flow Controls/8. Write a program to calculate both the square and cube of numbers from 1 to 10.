/* 8. Write a program to calculate both the square and cube of numbers from 1 to 10
1 1 1
2 4 8
3 9 27
. . .
. . .
10 100 1000 */

#include <iostream>
#include<cmath>
using namespace std; 
  
int main() 
{
 int num[10][3];
 for(int i=0;i<10;i++){
  for(int j=0;j<3;j++){
    num[i][j]=pow((i+1),(j+1));
    cout<<num[i][j]<<" ";
  }
  cout<<endl;
 }
     return 0;
  }
  
  
 
