//*6. Write the code to read elements of two-dimensional double array named sales. Then the 
program display the sum of the numbers stored in the first row, first column; the second row, 
third column; and the third row, fourth column */

#include <iostream>
using namespace std;
#include <string>
int main(){
    int balance[4][6];
	for(int i=0;i<4;i++){
		for(int j=0;j<6;j++){
			balance[i][j]=i+j;
			cout<<balance[i][j]<<" ";
		}
		cout<<endl;
	}
	return 0;
}
