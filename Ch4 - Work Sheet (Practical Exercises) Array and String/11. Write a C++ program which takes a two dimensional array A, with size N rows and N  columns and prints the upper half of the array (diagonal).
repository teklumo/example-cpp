/* 11. Write a C++ program which takes a two dimensional array A, with size N rows and N 
columns and prints the upper half of the array (diagonal).*/


#include <iostream>
using namespace std;
int main(){
      const int n=3;
      int arr[n][n];
      cout<<"enter the values:";
      for(int i=0;i<3;i++){
         for(int j=0;j<3;j++){
            cin>>arr[i][j];
         }

      }
      for(int i=0;i<3;i++){
         for(int j=0;j<3;j++){
            if(i<=j){
                cout<<arr[i][j]<<" ";
            }
            else{
                cout<<"  ";
            }
        }
        cout<<endl;
      }
    return 0;    

}
